 function calcularPromedio(a, b, c){
    return(a + b + c) / 3;
 }
 let promedio = calcularPromedio(4 ,7, 10);
 console.log(promedio);

 function encontrarMayor(a, b){
    return a > b ? a : b;
 }
 let numeroMayor  = encontrarMayor(15, 9);
    console.log(numeroMayor)
